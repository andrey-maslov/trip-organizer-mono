  "license": "MIT",
  "scripts": {
    "start:all": "nx run-many --parallel --target=serve --projects=backend,frontend-cra",
    "frontend:start": "nx serve frontend-cra",
    "backend:start": "nx serve frontend-cra",
    "test": "nx test",
    "lint": "nx lint",
    "backend:build:prod": "nx build backend --prod",
    "frontend:build:prod": "nx build frontend-cra --prod -- --memoryLimit=8192",
    "frontend:build:prod:stats": "nx build frontend-cra --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "frontend:build:dev:stats": "nx build frontend-cra --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "analyze:prod:stats": "run-s \"frontend:build:prod:stats\" \"webpack-bundle-analyzer\"",
    "analyze:dev:stats": "run-s \"frontend:build:dev:stats\" \"webpack-bundle-analyzer\"",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/frontend-cra/stats.json"
  },
  "engines": {
    "node": "18"
  },
  "private": true,

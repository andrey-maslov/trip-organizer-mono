{
  "name": "trip-organizer-mono",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start:all": "nx run-many --parallel --target=serve --projects=backend,frontend-cra",
    "frontend:start": "nx serve frontend-cra",
    "backend:start": "nx serve frontend-cra",
    "test": "nx test",
    "lint": "nx lint",
    "backend:build:prod": "nx build backend --prod",
    "frontend:build:prod": "nx build frontend-cra --prod -- --memoryLimit=8192",
    "frontend:build:prod:stats": "nx build frontend-cra --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "frontend:build:dev:stats": "nx build frontend-cra --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "analyze:prod:stats": "run-s \"frontend:build:prod:stats\" \"webpack-bundle-analyzer\"",
    "analyze:dev:stats": "run-s \"frontend:build:dev:stats\" \"webpack-bundle-analyzer\"",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/frontend-cra/stats.json"
  },
  "engines": {
    "node": "16.17.1"
  },
  "private": true,
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nrwl/cypress": "15.3.0",
    "@nrwl/eslint-plugin-nx": "15.3.0",
    "@nrwl/jest": "15.3.0",
    "@nrwl/linter": "15.3.0",
    "@nrwl/node": "15.3.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "15.3.0",
    "@nrwl/web": "15.3.0",
    "@nrwl/workspace": "15.3.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/react": "13.4.0",
    "@types/cors": "^2.8.13",
    "@types/jest": "28.1.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-router-dom": "5.3.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/ui": "^0.9.3",
    "babel-jest": "28.1.1",
    "cypress": "^11.0.0",
    "eslint": "~8.15.0",
